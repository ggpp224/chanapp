{
  "name": "webpack-boilerplate",
  "version": "0.0.3",
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js --config webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --progress --colors -p  && node sh.js",
    "server": "cross-env NODE_ENV=production node server.prod.js"
  },
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-redux": "4.0.0",
    "react-tap-event-plugin": "0.2.2",
    "immutable": "3.7.6",
    "chanjet-ui": ""
  },
  "keywords": [
    "yeoman-generator"
  ]
}